
<%- include("partials/header") -%> 

<h1>Compose</h1>


<form action="/" method="post">
    <div class="form-group">
      
    </div>
    <div class="form-group">
      <label for="titleInput">Title</label>
      <input class="form-control" type="text" name="postTitle" autocomplete="off" id="titleInput">
    </div>
    <div class="form-group">
        <label for="postInput">Post</label>
        <textarea class="form-control" id="postInput" rows="10" name="postBody"></textarea>
      </div>
<button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>


<%- include("partials/footer") -%> 